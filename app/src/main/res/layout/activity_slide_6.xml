<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            tools:context=".slide6Iterables.Slide6Activity">
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
    <Button android:id="@+id/basicRange"
                android:onClick="onClick"
                android:text="Basic Range"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"/>
        <Button android:id="@+id/halfRange"
                android:onClick="onClick"
                android:text="Half Closed Range"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"/>
        <Button android:id="@+id/steppedRange"
                android:onClick="onClick"
                android:text="Stepped Range"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"/>
        <Button android:id="@+id/countdownRange"
                android:onClick="onClick"
                android:text="Countdown Range"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"/>
        <Button android:id="@+id/basicList"
                android:onClick="onClick"
                android:text="Basic List"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"/>
        <Button android:id="@+id/mutableList"
                android:onClick="onClick"
                android:text="Mutable List"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"/>
        <Button android:id="@+id/generatedList"
                android:onClick="onClick"
                android:text="Generated List"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"/>
        <Button android:id="@+id/youngList"
                android:onClick="onClick"
                android:text="Young List"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"/>
        <Button android:id="@+id/testList"
                android:onClick="onClick"
                android:text="Test List"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"/>
        <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/listOutput"
                android:layout_margin="10dp"/>
    </LinearLayout>
</ScrollView>